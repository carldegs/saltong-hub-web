{
  "extends": [
    "eslint:recommended",
    "next/core-web-vitals",
    "eslint-config-prettier",
    "plugin:@typescript-eslint/recommended"
  ],
  "plugins": [
    "eslint-plugin-prettier",
    "@typescript-eslint"
  ],
  "rules": {
    "prettier/prettier": "error",
    "no-console": [
      "error",
      {
        "allow": [
          "warn",
          "error"
        ]
      }
    ],
    "no-unused-vars": "off"
  }
}